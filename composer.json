{
    "name": "paulmixxx/blog",
    "description": "Blog with DDD implementation.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul",
            "email": "paulmix.promo@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "league/event": "^3.0",
        "psr/event-dispatcher": "^1.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "infection/infection": "^0.18.2",
        "nunomaduro/phpinsights": "^1.14",
        "overtrue/phplint": "^2.1",
        "phpmd/phpmd": "^2.9",
        "phpmetrics/phpmetrics": "^2.7",
        "phpstan/phpstan": "^0.12.50",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Paulmixxx\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
